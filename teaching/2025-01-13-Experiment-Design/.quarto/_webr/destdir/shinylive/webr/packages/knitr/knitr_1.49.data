<<test-subfloat, echo=FALSE, out.width='.45\\linewidth'>>=
plot(1)
@

}\subfloat[another float]{
<<test-another, echo=FALSE, out.width='.45\\linewidth'>>=
plot(rnorm(100))
@

}

\caption{one figure with 2 sub figures\label{fig:knitr-subfloat}}
\end{figure}

